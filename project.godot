config_version=5

[application]

config/name="Theseus"
run/main_scene="res://game_manager.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon-temp.png"

[autoload]

Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"item": "res://dialogic/characters/item.dch",
"king": "res://dialogic/characters/king.dch",
"mechanic": "res://dialogic/characters/mechanic.dch",
"medic": "res://dialogic/characters/medic.dch",
"player": "res://dialogic/characters/player.dch"
}
directories/dtl_directory={
"end_cutscene": "res://dialogic/timelines/end_cutscene.dtl",
"intro_cutscene": "res://dialogic/timelines/intro_cutscene.dtl",
"medic_report": "res://dialogic/characters/medic_report.dtl"
}
variables={
"do_heal": true,
"player_health": 0.0,
"rest_days": 1.0,
"soul_percent": 100,
"soul_worth_factor": 0.0
}
layout/style_list=[]
layout/default_style="Default"
glossary/default_case_sensitive=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

